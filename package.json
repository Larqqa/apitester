{
  "name": "tauri-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "tauri dev",
    "dev": "set RUST_BACKTRACE=1 && concurrently -k \"npm run vite:dev\" \"npm run devtools\"",
